  <h2 class="official_name">{{name}}</h2>
  <div>
    <p class="official_capital">Capital: {{capital}}</p>
    <p class="official_population">Population: {{population}}</p>
    <p class="official_languages">Languages:</p>
    <ul class="lang-list">
        {{#each languages}}
        <li class="lang-list__item">{{this}}</li>
        {{/each }}
      </ul>
  </div>
  <img
      src="{{flag}}"
      alt="{{name}}"
      width="400"
      class="country__image"
    />
